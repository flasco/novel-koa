{
  "name": "novel-pricking",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "start": "npm build && node src/index",
    "lint": "eslint src",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "test-c": "yarn test --slient --coverage && open coverage/lcov-report/index.html",
    "build": "yarn tsc -p tsconfig.build.json && yarn tscpaths -p tsconfig.build.json -s ./src -o ./dist/src",
    "deploy": "trashbin run"
  },
  "engines": {
    "node": "14.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/async": "^3.2.5",
    "@types/cheerio": "^0.22.23",
    "@types/fs-extra": "^9.0.4",
    "@types/jest": "^26.0.19",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^14.14.10",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.6",
    "ora": "^5.1.0",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.4.3",
    "zan-trashbin": "^1.0.1-beta.1"
  },
  "dependencies": {
    "async": "^3.2.0",
    "axios": "^0.21.0",
    "cheerio": "^1.0.0-rc.3",
    "fs-extra": "^9.0.1",
    "iconv-lite": "^0.6.2",
    "koa-compose": "^4.1.0",
    "koa-ejs": "^4.3.0",
    "koa-static": "^5.0.0",
    "koa-views": "^7.0.0",
    "leancloud-storage": "^4.8.2",
    "leanengine": "^3.7.0",
    "lru-cache": "^6.0.0",
    "pricking-koa": "0.0.2-alpha.9",
    "tslib": "^2.3.1",
    "urlencode": "^1.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": "eslint"
  }
}
